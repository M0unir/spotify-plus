{
  "name": "spotify-plus",
  "version": "1.0.0",
  "description": "web app to visualize personalized Spotify Data",
  "main": "index.js",
  "engines": {
    "node": "14.15.0"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "scripts": {
    "start": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "server": "nodemon index.js",
    "client": "cd client && yarn start",
    "postinstall": "cd client && yarn install",
    "build": "NODE_ENV=production cd client/ && yarn install & yarn run build",
    "start-server": "node index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/M0unir/spotify-plus.git"
  },
  "keywords": [
    "react",
    "spotify"
  ],
  "author": "Mounir Chouladi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/M0unir/spotify-plus/issues"
  },
  "homepage": "https://github.com/M0unir/spotify-plus#readme",
  "dependencies": {
    "axios": "^0.22.0",
    "concurrently": "^6.3.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "qs": "^6.10.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.13"
  }
}
